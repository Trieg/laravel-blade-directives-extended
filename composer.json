{
  "name": "periloso/laravel-blade-directives-extended",
  "description": "Directive Extensions to Laravel Blade templating engine.",
  "license": "MIT",
  "authors": [
    {
      "name": "Alessio Periloso",
      "email": "mail@periloso.com"
    }
  ],
  "require": {
    "laravel/framework": "~5.0",
    "php": ">=5.4.0"
  },
  "autoload": {
    "psr-4": {
      "Periloso\\BladeDirectivesExtended\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Periloso\\BladeDirectivesExtended\\BladeDirectivesExtendedServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
